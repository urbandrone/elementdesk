(use "./data/widgets" get-all get-by-tags get-by-id get-by-name)

(defun get-widgets ()
  (get-all))

(defun get-widgets-by-tags (tags)
  (get-by-tags ...tags))

(defun get-widget-by-id (id)
  (get-by-id id))

(defun get-widget-by-name (name)
  (get-by-name name))

(provide-all get-widgets
             get-widgets-by-tags
             get-widget-by-id
             get-widget-by-name) 
  
