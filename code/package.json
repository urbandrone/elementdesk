{
  "name": "element-desk",
  "version": "0.0.1",
  "description": "A desktop app for website design",
  "main": "boot.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "compile": "sibilisp -s src/ -d compiled/ -f js",
    "build": "rollup -c configs/rollup.config.js",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbandrone/elementdesk.git"
  },
  "keywords": [
    "Design",
    "Website",
    "Electron",
    "Tool"
  ],
  "author": "David Hofmann <the.urban.drone@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/urbandrone/elementdesk/issues"
  },
  "homepage": "https://github.com/urbandrone/elementdesk#readme",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "electron": "^15.0.0",
    "electron-builder": "^22.11.7",
    "rollup": "^2.57.0",
    "rollup-plugin-filesize": "^9.1.1",
    "sibilisp": "^0.6.8"
  },
  "dependencies": {
    "better-sqlite3": "^7.4.3",
    "fs-extra": "^10.0.0",
    "superfine": "^8.2.0"
  }
}
